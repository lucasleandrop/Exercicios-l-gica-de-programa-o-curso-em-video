algoritmo "Ex10CursoEmVideo"
var
    a, l, area, lata : real
inicio
     Escreva("Qual a altura da parede a ser pintada? ")
     Leia(a)
     Escreva("Qual a largura da parede a ser pintada? ")
     Leia(l)
     area <- a * l
     lata <- area / 2
     Escreval("Você terá ", area, " metros quadrados de area para pintar, e usará ", lata, " litros de tinta")
fimalgoritmo