algoritmo "Ex11CursoEmVideo"
var
   A, B, C, D : real
inicio
   Escreva("Digite o valor de A ")
   Leia(A)
   Escreva("Digite o valor de B ")
   Leia(B)
   Escreva("Digite o valor de C ")
   Leia(C)
      D <- exp(B, 2) - 4*A*C
      Escreval("O DELTA desta equação de segundo grau é ", D)
fimalgoritmo