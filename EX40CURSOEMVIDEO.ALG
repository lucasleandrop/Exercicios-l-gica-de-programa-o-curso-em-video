algoritmo "Ex40CursoEmVideo"
var
 c : inteiro
inicio
    c <- 0
      Escreval("Contador de 3 em 3 até 18")
      Escreval("-------------------------")
      Enquanto c < 19 faca
               Escreva(" ", c, " ")
               c <- c + 3
      FimEnquanto
              Escreva(" ACABOU!")
fimalgoritmo