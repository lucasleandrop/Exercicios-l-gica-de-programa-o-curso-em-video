algoritmo "Ex61CursoEmVideo"
var
    c : inteiro
inicio
c <- 0
      Escreval("Contagem faca enquanto")
      Escreval("----------------------")
      Escreval()
      Enquanto (c < 31) faca
               Escreva(" ", c, "")
               c <- c + 3
      FimEnquanto
      Escreva(".Acabou!")
fimalgoritmo