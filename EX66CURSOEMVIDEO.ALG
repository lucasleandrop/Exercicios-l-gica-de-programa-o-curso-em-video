algoritmo "Ex66CursoEmVideo"
var
    n, c : inteiro
inicio
       Escreval("Tabuada, usando para")
       Escreval("--------------------")
       Escreval()
       Escreval("Digite um número para saber sua tabuada : ")
       Leia(n)
       Escreval()
       Para c <- 1 ate 10 faca
            Escreval(n * c)
       FimPara
fimalgoritmo