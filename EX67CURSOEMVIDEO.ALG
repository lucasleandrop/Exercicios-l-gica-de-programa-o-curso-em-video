algoritmo "Ex67CursoEmVideo"
var
     c,  n : inteiro
inicio
      Escreval("Contagem do número a escolha")
      Escreval("----------------------------")
      Escreval()
      Escreva("Digite um número inteiro para a contagem : ")
      Leia(n)
      Para c <- 0 ate n faca
           Escreva(c)
      FimPara
      Escreva(" FIM!")

      
fimalgoritmo