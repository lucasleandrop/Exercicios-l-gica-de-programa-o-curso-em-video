algoritmo "Ex70CursoEmVideo"
var
   t1, t2, t3, c : inteiro
inicio
     Escreval("Sequência Fibonacci 10 termos")
     Escreval("-----------------------------")
     Escreval()
     t1 <- 0
     Escreva(t1)
     t2 <- 1
     Escreva(t2)
     Para c <- 3 ate 10 faca
     t3 <- t1 + t2
     Escreva(t3)
     t1 <- t2
     t2 <- t3
     FimPara
fimalgoritmo
