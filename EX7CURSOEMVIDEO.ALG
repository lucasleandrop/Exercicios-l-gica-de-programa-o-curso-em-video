algoritmo "Ex7CursoemVideo"
var
    n, nd, n13 : real
inicio
        Escreva("Digite um número para saber o valor do seu dobro e do seu um terço ")
        Leia(n)
        nd <- n + n
        n13 <- n / 3
        Escreval("o dobro do número ", n, " é: ", nd, " e um terço do valor do mesmo é : ", n13)
fimalgoritmo